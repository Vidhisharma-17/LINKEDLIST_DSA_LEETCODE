class Solution {
public:
    bool isPalindrome(ListNode* head) {
        vector<int> values;

        ListNode* curr=head;

        while(curr){
            values.push_back(curr->val);
            curr=curr->next;
        }

        int i=0;
        int j=values.size()-1;

        while(i<j){
            if(values[i]!=values[j]){
                return false;
            }
            i++;
            j--;
        }
        return true;
        
        
    }
};
